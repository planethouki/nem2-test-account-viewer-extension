{
  "name": "nem2-test-account-viewer-extension",
  "manifest_version": 2,
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon_19.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "tabs"
  ],
  "externally_connectable": {
    "matches": [
      "https://www.google.com/*"
    ],
    "ids": [
      "*"
    ]
  }
}
